@model PTemplate.Data.DataBase.Productmenuinfo


@using Microsoft.AspNetCore.Identity

@using PTemplate.Areas.Admin.Data
@using PTemplate.Models
@namespace PTemplate


@using PTemplate
@using PTemplate.Models

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "حذف زیر منو";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>حذف</h2>

<h3>ایا مایل به حذف این اطلاعات هستید؟?</h3>
<div>
    <h4>حذف اطلاعات منو</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           عنوان
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Title)
        </dd>
        <dt>
          تاریخ درج
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Date)
        </dd>
        <dt>
           شماره
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Number)
        </dd>
        <dt>
           منو
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdsubmenuNavigation.Name)
        </dd>
    </dl>

    <form  action="~/Admin/productmenuinfoes/Delete" method="post" >
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="حذف" class="btn btn-default" /> |
        <a href="~/Admin/productmenuinfoes/Index">برگشت</a>
    </form>
</div>
